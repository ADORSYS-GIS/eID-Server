               <SubjectURL>https://your-eservice.example.com</SubjectURL>
               <CommCertificates></CommCertificates>
            </ecard:CertificateDescription>
            <ecard:RequiredCHAT>read_name,read_address,age_verification</ecard:RequiredCHAT>
            <ecard:OptionalCHAT>write_identity</ecard:OptionalCHAT>
            <ecard:AuthenticatedAuxiliaryData>TestTransactionInfo</ecard:AuthenticatedAuxiliaryData>
         </ecard:AuthenticationProtocolData>
      </ecard:DIDAuthenticate>
   </soapenv:Body>
</soapenv:Envelope>